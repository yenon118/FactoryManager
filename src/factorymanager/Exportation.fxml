<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="TOP_CENTER" prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="factorymanager.ExportationController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" text="Exportation" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="100.0" />
         </VBox.margin>
      </Label>
      <HBox>
         <children>
            <Label text="Company Name: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="companyName" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="40.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Point of Contact: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="pointOfContact" prefWidth="350.0" />
            <Label text="Email: ">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="email" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Country Code: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="countryCode" prefWidth="300.0" />
            <Label text="Phone Number: ">
               <HBox.margin>
                  <Insets left="50.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="phoneNumber" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Address: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="address" prefWidth="350.0" />
            <Label text="City: ">
               <HBox.margin>
                  <Insets left="70.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="city" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="State: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="state" prefWidth="350.0" />
            <Label text="Zip Code: ">
               <HBox.margin>
                  <Insets left="90.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="zipCode" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Date: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="date" prefWidth="350.0" />
            <Label text="Product: ">
               <HBox.margin>
                  <Insets left="90.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="product" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox>
         <children>
            <Label text="Price per Unit: ">
               <HBox.margin>
                  <Insets left="30.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="pricePerUnit" prefWidth="300.0" />
            <Label text="Quantity: ">
               <HBox.margin>
                  <Insets left="90.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="quantity" prefWidth="350.0" />
         </children>
         <padding>
            <Insets top="10.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <Label fx:id="invalidTextInTextField" textFill="RED">
         <VBox.margin>
            <Insets left="20.0" right="20.0" top="30.0" />
         </VBox.margin>
      </Label>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#clickCancelButton" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="submitButton" mnemonicParsing="false" onAction="#clickSubmitButton" text="Submit">
               <font>
                  <Font size="18.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets left="50.0" right="50.0" top="30.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
